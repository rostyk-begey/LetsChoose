{
  "npmScope": "lets-choose",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "test", "lint", "package", "prepare"],
        "accessToken": "Y2U2MTU0NWMtMTcxNC00ZTVjLTkxZjUtYzdlZGNlMjQzYzMyfHJlYWQtd3JpdGU=",
        "parallel": 1
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": false
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "none",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "client",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "prepare": {
      "dependsOn": ["^prepare"]
    },
    "package": {
      "dependsOn": ["^package"]
    }
  }
}
