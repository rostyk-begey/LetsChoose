{
  "version": 2,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "targets": {
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx build api",
              "docker build -f ./apps/api/Dockerfile . -t api"
            ],
            "parallel": false
          }
        },
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-auth-feature": {
      "root": "libs/api/auth/feature",
      "sourceRoot": "libs/api/auth/feature/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/api/auth/feature/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/auth/feature"],
          "options": {
            "jestConfig": "libs/api/auth/feature/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-health": {
      "root": "libs/api/health",
      "sourceRoot": "libs/api/health/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/api/health/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/health"],
          "options": {
            "jestConfig": "libs/api/health/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-user-data-access": {
      "root": "libs/api/user/data-access",
      "sourceRoot": "libs/api/user/data-access/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/api/user/data-access/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/user/data-access"],
          "options": {
            "jestConfig": "libs/api/user/data-access/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "client": {
      "root": "apps/client",
      "sourceRoot": "apps/client",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/client",
            "outputPath": "dist/apps/client",
            "generatePackageJson": true
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "client:build",
            "dev": true,
            "port": 3000,
            "proxyConfig": "apps/client/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "client:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "client:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/client"],
          "options": {
            "jestConfig": "apps/client/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/client/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "client-api": {
      "root": "libs/client/api",
      "sourceRoot": "libs/client/api/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/client/api/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client/api"],
          "options": {
            "jestConfig": "libs/client/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "client-components": {
      "root": "libs/client/components",
      "sourceRoot": "libs/client/components/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/client/components/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client/components"],
          "options": {
            "jestConfig": "libs/client/components/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/client/components/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/client-components",
            "config": {
              "configFolder": "libs/client/components/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "client-components-e2e": {
      "root": "apps/client-components-e2e",
      "sourceRoot": "apps/client-components-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/client-components-e2e/cypress.json",
            "devServerTarget": "client-components:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "client-components:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/client-components-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "client-e2e": {
      "root": "apps/client-e2e",
      "sourceRoot": "apps/client-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/client-e2e/cypress.json",
            "devServerTarget": "client:serve",
            "tsConfig": "apps/client-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "client:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/client-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "client-hooks": {
      "root": "libs/client/hooks",
      "sourceRoot": "libs/client/hooks/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/client/hooks/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client/hooks"],
          "options": {
            "jestConfig": "libs/client/hooks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "client-pages": {
      "root": "libs/client/pages",
      "sourceRoot": "libs/client/pages/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/client/pages/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client/pages"],
          "options": {
            "jestConfig": "libs/client/pages/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "client-utils": {
      "root": "libs/client/utils",
      "sourceRoot": "libs/client/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/client/utils/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client/utils"],
          "options": {
            "jestConfig": "libs/client/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "common-dto": {
      "root": "libs/common/dto",
      "sourceRoot": "libs/common/dto/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/common/dto/**/*.ts"]
          }
        }
      }
    },
    "common-utils": {
      "root": "libs/common/utils",
      "sourceRoot": "libs/common/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/common/utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/common/utils"],
          "options": {
            "jestConfig": "libs/common/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "client"
}
