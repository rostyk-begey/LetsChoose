{
  "presets": [
    [
      "@nrwl/next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    [
      "babel-plugin-direct-import",
      { "modules": ["@mui/material", "@mui/icons-material"] }
    ]
  ]
}
